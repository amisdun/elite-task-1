{
  "name": "elite-auto",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-syntax-decorators": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@electric-sql/pglite": "^0.2.17",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "node-mocks-http": "^1.16.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.1",
    "typeorm-pglite": "^0.3.2",
    "typescript": "^5.7.3",
    "@types/supertest": "^6.0.2",
    "@types/emscripten": "^1.40.0",
    "@types/web": "^0.0.203"
  },
  "dependencies": {
    "@": "types/express",
    "@types/node-cron": "^3.0.11",
    "body-parser": "^1.20.3",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "node-cron": "^3.0.3",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.20"
  },
  "type": "commonjs",
  "scripts": {
    "watch": "tsc -w",
    "dev": "npm run watch && nodemon build/index.js",
    "start:dev": "NODE_ENV=dev && concurrently \"tsc -w\" \"nodemon build/index.js\"",
    "build": "tsc",
    "start": "ts-node src/index.ts -w",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration": " npm run typeorm migration:run -- -d ./src/data-source.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "NODE_ENV=test && node --experimental-vm-modules node_modules/.bin/jest --detectOpenHandles"
  }
}
