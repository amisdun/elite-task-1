      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "types": ["@types/emscripten", "@types/web", "@types/jest"]
   },
   "exclude": ["jest.config.ts"]
}